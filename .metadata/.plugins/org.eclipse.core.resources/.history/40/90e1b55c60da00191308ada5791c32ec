#include <stdio.h>
#include <stdlib.h>

int main(void) {

	typedef struct {
		char nombre[50];
		char apellido[50];
		char calle[50];
		int numero[50];

	}datosPersonales;

	struct datosPersonales vecAgenda[3], vecContactos[3], vecAmigos[3];

	//vecAgenda[0].nombre

	return 0;
}



