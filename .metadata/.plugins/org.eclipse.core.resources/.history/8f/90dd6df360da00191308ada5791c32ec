#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct {
	char nombre[50];
	char apellido[50];
	char calle[50];
	int numero[50];

	}datosPersonales;

int main(void) {

	struct datosPersonales vecAgenda[3], vecContactos[3], vecAmigos[3];
	int i;

	for(i=0;i<3;i++)
	{
		printf("\nIngrese nombre: ");
		gets(vecAgenda[i].nombre);
	}


	//vecAgenda[0].nombre
	//strcopy(vecAgend[0].nombre,"nombreDeLaPersona");

	return 0;
}



